{
  "name": "now.js",
  "version": "0.1.0",
  "description": "now is a time toolkit for javascript",
  "main": "./dist/nowjs.js",
  "scripts": {
    "start": "concurrently --kill-others  \"npm run serve\" \"npm run build -- -w\"",
    "serve": "gulp serve",
    "test": "jest",
    "prepublish": "npm run build",
    "build": "node build.js",
    "lint": "./node_modules/.bin/eslint --fix ./src"
